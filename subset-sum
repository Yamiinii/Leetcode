#include <bits/stdc++.h> 


bool subsetSumToK(int n, int k, vector<int> &arr) {
    // Write your code here.
   vector<vector<bool>> dp(n + 1, vector<bool>(k + 1, false));
    
    for(int i = 0; i <= n; i++) {
        for(int j = 0; j <= k; j++) {
            if(j == 0) {
                dp[i][j] = true;
            } else if(i == 0) {
                dp[i][j] = false;
            } else if(arr[i - 1] <= j) {
                dp[i][j] = dp[i - 1][j - arr[i - 1]] || dp[i - 1][j];
            } else {
                dp[i][j] = dp[i - 1][j];
            }
        }
    }
    
    return dp[n][k];

}
